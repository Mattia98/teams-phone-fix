{
  "description": "Fixes Microsoft Teams phone functionality by using the user agent from Microsoft Edge.",
  "manifest_version": 2,
  "name": "Teams Phone Fix",
  "version": "1.0",
  "homepage_url": "https://github.com/dev-rke/teams-phone-fix",
  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://teams.microsoft.com/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
